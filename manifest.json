{
  "manifest_version": 2,
	
  "name": "TakeNote",
  "description": "Notifies you about upcoming contests",
  "background": {"scripts" : ["bg.js"]},
  "version": "1.0",

 
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "./popup.html"
  },
  
  "permissions":["http://codeforces.com/api/contest.list","tabs"]
}
